{
  "name": "proj-inject",
  "description": "Utility to inject standardized or templated content into projects.",
  "version": "0.1.0",
  "author": "Brennan Fee",
  "bin": {
    "proj-inject": "./bin/run"
  },
  "bugs": "https://github.com/brennanfee/proj-inject/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@tsconfig/node14": "^1.0.0",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "chai": "^4.3.0",
    "eslint": "^7.20.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-config-prettier": "^7.2.0",
    "globby": "^11.0.2",
    "mocha": "^8.3.0",
    "nyc": "^15.1.0",
    "prettier": "2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/brennanfee/proj-inject",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "proj-inject",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "brennanfee/proj-inject",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc.yml",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha -r ts-node/register --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "tslib": "^2.1.0"
  }
}
